<h1>log in</h1>

<%= form_tag  sessions_path do %>
	<div class="field">
	<%= label_tag :usuario %>
	
	<%= text_field_tag :usuario, params[:usuario]%>	
	
	
	</div>
	
	<div class="field">
	<%=label_tag :password %>
	<%=password_field_tag :password %>
	</div>
	<div class="actions"><%=submit_tag "Log in", :class =>"boton" %></div>
	
<% end %>
