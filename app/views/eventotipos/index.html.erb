<h1>Lista de eventotipos</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
			<th>Nombre</th>
			<th>Unidad</th>
      <th>Desc</th>
      <th></th>
      <th></th>  
    </tr>
  </thead>

  <tbody>
    <% @eventotipos.each do |eventotipo| %>
      <tr>
        <td><%= eventotipo.codigo %></td>
        <td><%= eventotipo.nombre %></td>
        <td><%= eventotipo.unidad %></td>
        <td><%= eventotipo.desc %></td>
         <td><%= link_to image_tag("editar.png", :height => 20, :border=>0), edit_eventotipo_path(eventotipo) %></td>
        <td><%= link_to image_tag("borrar.png", :height => 20, :border=>0), eventotipo, method: :delete, data: { confirm: 'Esta seguro que desea borrar el registro?' } %></td>
  </tr>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Crear Eventotipo', new_eventotipo_path %>
